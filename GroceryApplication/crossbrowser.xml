<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Grocery Application Suite" parallel="tests">
  <test thread-count="5" name="Chrome Test">
	  
	  <parameter name="browser" value="chrome"></parameter>
	   
    <classes>
      <class name="testCases.DashboardTestcases"/>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="Edge Test">
	  
	  <parameter name="browser" value="edge"></parameter>
	   
    <classes>
      <class name="testCases.DashboardTestcases"/>
      <class name="testCases.AddPageTestcases"/>
      <class name="testCases.EditPageTestcases"/>
      <class name="testCases.LoginPageTestcases"/>
      <class name="testCases.ManageExpenseTestcases"/>
      <class name="testCases.ManagePagesListTestcases"/>
      <class name="testCases.ManageProductTestcases"/> 
      <class name="testCases.NavigateBetweenPagesTestcases"/> 
    </classes>
  </test> <!-- Test -->
  
  
</suite> <!-- Suite -->
